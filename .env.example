# Google Places API (for autocomplete, map features)
VITE_GOOGLE_PLACES_API_KEY=your_google_places_api_key_here

# Google Maps API (for map rendering)
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Google Gemini AI (DEPRECATED - moved to backend for security)
# Only needed if VITE_USE_GEMINI_PROXY=false (not recommended)
VITE_GOOGLE_GEMINI_AI_API_KEY=your_google_gemini_api_key_here

# Google OAuth (for user authentication)
VITE_GOOGLE_AUTH_CLIENT_ID=your_google_oauth_client_id_here

# Backend API URL
VITE_API_BASE_URL=http://localhost:8000

# Gemini Proxy Configuration (RECOMMENDED: keep as true)
VITE_USE_GEMINI_PROXY=true